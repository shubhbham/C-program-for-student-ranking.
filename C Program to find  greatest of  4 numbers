#include <stdio.h>

int main()
{
    float num1, num2, num3, num4, num;
    printf("Enter the number 1 \n");
    scanf("%f", &num1);

    printf("Enter the number 2 \n");
    scanf("%f", &num2);

    printf("Enter the number 3 \n");
    scanf("%f", &num3);

    printf("Enter the number 4 \n");
    scanf("%f", &num4);

    if ((num1 >= num2) && (num1 >= num3) && (num1 >= num4))
    {
        printf("num 1 is great %f\n",num1);
    }

    if ((num2 >= num1) && (num2 >= num3) && (num2 >= num4))
    {
        printf("num 2 is great %f\n",num2);
    }

    if ((num3 >= num1) && (num3 >= num2) && (num3 >= num4))
    {
        printf("num 3 is great %f\n",num3);
    }
    if ((num4 >= num1) && (num4 >= num2) && (num4 >= num3))
    {
        printf("num 4 is great %f\n",num4);
    }

    return 0;
}
